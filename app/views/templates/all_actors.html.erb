<table>
  <tr>
    <th>ID</th>
    <th>Image</th>
    <th>Name</th>
    <th>Dob</th>
    <th>Actions</th>
  </tr>
  <% @actors.each do |actor| %>
    <tr>
      <td><%= actor.id %></td>
      <td>
       <img src= <%= actor.image %> style="width:250px;">
      </td>
      <td><%= actor.name %></td>
      <td><%= actor.dob %></td>
      <td>
      <a href="/actors/<%= actor.id %>">
        Show details
      </a>
      </td>
    </tr>
  <% end %>
</table>
