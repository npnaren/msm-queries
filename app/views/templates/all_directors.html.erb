<table>
  <tr>
    <th>ID</th>
    <th>Image</th>
    <th>Name</th>
    <th>Dob</th>
    <th>Actions</th>
  </tr>
  <% @directors.each do |director| %>
    <tr>
      <td><%= director.id %></td>
      <td>
       <img src= <%= director.image %> style="width:250px;">
      </td>
      <td><%= director.name %></td>
      <td><%= director.dob %></td>
      <td>
      <a href="/directors/<%= director.id %>">
        Show details
      </a>
      </td>
    </tr>
  <% end %>
</table>
